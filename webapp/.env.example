# XR-Swarm-Bridge Environment Configuration

# OpenAI GPT-4o API Configuration
REACT_APP_OPENAI_API_KEY=your_openai_api_key_here

# Swarm Coordinator Configuration
REACT_APP_SWARM_WS_URL=ws://localhost:8443
REACT_APP_SWARM_HTTP_URL=http://localhost:8080

# WebRTC Configuration
REACT_APP_WEBRTC_STUN_SERVERS=stun:stun.l.google.com:19302
REACT_APP_WEBRTC_TURN_SERVER=
REACT_APP_WEBRTC_TURN_USERNAME=
REACT_APP_WEBRTC_TURN_PASSWORD=

# Development Configuration
REACT_APP_DEBUG_MODE=true
REACT_APP_LOG_LEVEL=info

# VR/AR Configuration
REACT_APP_VR_ENABLED=true
REACT_APP_AR_ENABLED=true
REACT_APP_HAND_TRACKING=true

# Performance Configuration
REACT_APP_MAX_ROBOTS=100
REACT_APP_UPDATE_RATE=30
REACT_APP_VIDEO_QUALITY=medium

# Security Configuration
REACT_APP_ENABLE_SSL=false
REACT_APP_SSL_CERT_PATH=
REACT_APP_SSL_KEY_PATH=

# Analytics (Optional)
REACT_APP_ANALYTICS_ENABLED=false
REACT_APP_ANALYTICS_ENDPOINT=

# Network Configuration
REACT_APP_MAX_RECONNECT_ATTEMPTS=5
REACT_APP_RECONNECT_INTERVAL=3000
REACT_APP_HEARTBEAT_INTERVAL=5000

# UI Configuration
REACT_APP_THEME=dark
REACT_APP_SHOW_DEBUG_STATS=true
REACT_APP_DEFAULT_VIEW_MODE=3d